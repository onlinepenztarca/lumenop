{
    "name": "onlinepenztarca/lumenop",
    "description": "onlinePenztarca lumen osszekottetes",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
		"onlinepenztarca/opclient": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
